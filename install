local player = game.Players.LocalPlayer
local backpack = player:FindFirstChild("Backpack") or player:WaitForChild("Backpack")

-- ðŸ“Œ HÃ m Ä‘áº¿m sá»‘ lÆ°á»£ng Insta-Plant Capsule
function countCapsules()
    local capsuleCount = 0
    
    for _, item in ipairs(backpack:GetChildren()) do
        if item:IsA("Tool") and string.match(item.Name, "Insta-Plant Capsule") then
            capsuleCount = capsuleCount + 1
        end
    end

    return capsuleCount
end

-- ðŸš€ In sá»‘ lÆ°á»£ng Insta-Plant Capsule ra console
while true do
    local count = countCapsules()
    print("ðŸŸ¢ Sá»‘ lÆ°á»£ng Insta-Plant Capsule trong inventory:", count)
    wait(5) -- Cáº­p nháº­t má»—i 5 giÃ¢y (cÃ³ thá»ƒ chá»‰nh)
end
