local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- 📌 Tọa độ đích cần teleport đến
local targetPosition = Vector3.new(-465.9693908691406, 108.42337036132812, -1399.500244140625)

-- 📌 Teleport nhân vật mà không thay đổi bất kỳ thông số nào của camera
humanoidRootPart.CFrame = CFrame.new(targetPosition)

print("✅ Teleport thành công mà không ảnh hưởng đến camera!")
