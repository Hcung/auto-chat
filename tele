local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

-- ğŸ“Œ Tá»a Ä‘á»™ Ä‘iá»ƒm trÆ°á»›c cá»•ng vÃ o map
local startPosition = Vector3.new(184.9508819580078, 22.715150833129883, 1995.9241943359375)

-- ğŸ“Œ Tá»a Ä‘á»™ Ä‘iá»ƒm tÆ°á»›i cÃ¢y (ÄÃ£ thay báº±ng tá»a Ä‘á»™ má»›i)
local targetPosition = Vector3.new(-450.8958740234375, 108.41856384277344, -1392.82421875)

-- ğŸ“Œ HÃ m teleport nhÃ¢n váº­t Ä‘áº¿n trÆ°á»›c cá»•ng
local function teleportToStart()
    if humanoidRootPart then
        humanoidRootPart.CFrame = CFrame.new(startPosition)
        print("âœ… ÄÃ£ teleport Ä‘áº¿n trÆ°á»›c cá»•ng vÃ o map!")
    end
end

-- ğŸ“Œ Teleport ngay khi vÃ o game Ä‘áº¿n trÆ°á»›c cá»•ng map
teleportToStart()

-- ğŸ“Œ Chá» 1 giÃ¢y rá»“i di chuyá»ƒn vÃ o map
task.wait(1)

-- ğŸ“Œ Di chuyá»ƒn nhÃ¢n váº­t vá» phÃ­a trÆ°á»›c (3-4 bÆ°á»›c)
local moveDirection = Vector3.new(0, 0, -5) -- Äiá»u chá»‰nh vector Ä‘á»ƒ Ä‘i Ä‘Ãºng hÆ°á»›ng vÃ o map
for i = 1, 4 do
    humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(moveDirection)
    task.wait(0.5) -- Äá»£i Ä‘á»ƒ trÃ¡nh lá»—i dá»‹ch chuyá»ƒn quÃ¡ nhanh
end
print("ğŸš¶â€â™‚ï¸ NhÃ¢n váº­t Ä‘Ã£ vÃ o map!")

-- ğŸ“Œ Chá» 2 giÃ¢y sau khi vÃ o map rá»“i teleport Ä‘áº¿n chá»— tÆ°á»›i cÃ¢y
task.wait(2)
humanoidRootPart.CFrame = CFrame.new(targetPosition)
print("âœ… Teleport Ä‘áº¿n vá»‹ trÃ­ tÆ°á»›i cÃ¢y!")

-- ğŸ“Œ Chá»‰nh camera cao lÃªn vÃ  lÃ¹i ra sau má»™t chÃºt Ä‘á»ƒ trÃ¡nh bá»‹ che khuáº¥t
local camera = game.Workspace.CurrentCamera
task.wait(0.5) -- Äá»£i má»™t chÃºt Ä‘á»ƒ trÃ¡nh lá»—i khi camera chÆ°a load xong
camera.CFrame = camera.CFrame * CFrame.new(0, 10, -10) -- Äáº©y camera lÃªn cao hÆ¡n 10 Ä‘Æ¡n vá»‹ vÃ  lÃ¹i ra sau 10 Ä‘Æ¡n vá»‹

-- ğŸ“Œ HÃ m tá»± Ä‘á»™ng tÆ°á»›i cÃ¢y (tÆ°á»›i má»—i cÃ¢y 4 láº§n)
local function waterPlants()
    local plantPositions = {
        Vector3.new(-452.69903564453125, 107.31900787353516, -1400.0970458984375),
        Vector3.new(-435.25531005859375, 107.33667755126953, -1399.5416259765625),
        Vector3.new(-435.50177001953125, 107.31939697265625, -1387.158447265625),
        Vector3.new(-435.40069580078125, 107.31939697265625, -1412.1187744140625)
    }

    for _, pos in ipairs(plantPositions) do
        for i = 1, 4 do -- TÆ°á»›i 4 láº§n trÆ°á»›c khi chuyá»ƒn cÃ¢y khÃ¡c
            local args = {
                [1] = Ray.new(Vector3.new(-478.3576354980469, 109.90526580810547, -1400.040283203125), pos - Vector3.new(-478.3576354980469, 109.90526580810547, -1400.040283203125)),
                [2] = pos
            }
            game:GetService("ReplicatedStorage").Network.Click:FireServer(unpack(args))
            task.wait(0.5) -- Chá» 0.5 giÃ¢y giá»¯a má»—i láº§n click Ä‘á»ƒ trÃ¡nh lag
        end
        print("ğŸŒ± ÄÃ£ tÆ°á»›i xong 4 láº§n cho cÃ¢y táº¡i:", pos)
    end
    print("ğŸŒ¿ ÄÃ£ tÆ°á»›i xong toÃ n bá»™ cÃ¢y!")
end

-- ğŸ“Œ Tá»± Ä‘á»™ng tÆ°á»›i cÃ¢y ngay khi teleport Ä‘áº¿n vá»‹ trÃ­ tÆ°á»›i
task.wait(1)
waterPlants()

-- ğŸ“Œ Kiá»ƒm tra náº¿u nhÃ¢n váº­t bá»‹ rá»›t khá»i map, tá»± Ä‘á»™ng teleport vá» láº¡i trÆ°á»›c cá»•ng
task.spawn(function()
    while task.wait(2) do -- Kiá»ƒm tra má»—i 2 giÃ¢y
        if humanoidRootPart and humanoidRootPart.Position.Y < 10 then -- Náº¿u rÆ¡i xuá»‘ng quÃ¡ tháº¥p
            print("âš ï¸ PhÃ¡t hiá»‡n nhÃ¢n váº­t rá»›t khá»i map! Äang teleport láº¡i trÆ°á»›c cá»•ng...")
            teleportToStart()
        end
    end
end)
