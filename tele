local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- ğŸ“Œ Cáº­p nháº­t láº¡i vá»‹ trÃ­ teleport (tá»a Ä‘á»™ má»›i)
local targetPosition = Vector3.new(-465.9693908691406, 108.42337036132812, -1399.500244140625)

-- ğŸ“Œ Camera Lock (tá»« Dark Dex)
local camera = workspace.CurrentCamera
camera.CameraType = Enum.CameraType.Scriptable
camera.CFrame = CFrame.new(473.877365, 116.042824, -1396.968994)

-- ğŸ“Œ Teleport nhÃ¢n váº­t Ä‘áº¿n vá»‹ trÃ­ mong muá»‘n
humanoidRootPart.CFrame = CFrame.new(targetPosition)

