local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

-- ğŸ“Œ Tá»a Ä‘á»™ Ä‘iá»ƒm trÆ°á»›c cá»•ng vÃ o map
local startPosition = Vector3.new(184.9508819580078, 22.715150833129883, 1995.9241943359375)

-- ğŸ“Œ Tá»a Ä‘á»™ Ä‘iá»ƒm tÆ°á»›i cÃ¢y
local targetPosition = Vector3.new(-465.9693908691406, 108.42337036132812, -1399.500244140625)

-- ğŸ“Œ HÃ m teleport nhÃ¢n váº­t Ä‘áº¿n trÆ°á»›c cá»•ng
local function teleportToStart()
    if humanoidRootPart then
        humanoidRootPart.CFrame = CFrame.new(startPosition)
        print("âœ… ÄÃ£ teleport Ä‘áº¿n trÆ°á»›c cá»•ng vÃ o map!")
    end
end

-- ğŸ“Œ Teleport ngay khi vÃ o game Ä‘áº¿n trÆ°á»›c cá»•ng map
teleportToStart()

-- ğŸ“Œ Chá» 1 giÃ¢y rá»“i di chuyá»ƒn vÃ o map
task.wait(1)

-- ğŸ“Œ Di chuyá»ƒn nhÃ¢n váº­t vá» phÃ­a trÆ°á»›c (3-4 bÆ°á»›c)
local moveDirection = Vector3.new(0, 0, -5) -- Äiá»u chá»‰nh vector Ä‘á»ƒ Ä‘i Ä‘Ãºng hÆ°á»›ng vÃ o map
for i = 1, 4 do
    humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(moveDirection)
    task.wait(0.5) -- Äá»£i Ä‘á»ƒ trÃ¡nh lá»—i dá»‹ch chuyá»ƒn quÃ¡ nhanh
end
print("ğŸš¶â€â™‚ï¸ NhÃ¢n váº­t Ä‘Ã£ vÃ o map!")

-- ğŸ“Œ Chá» 2 giÃ¢y sau khi vÃ o map rá»“i teleport Ä‘áº¿n chá»— tÆ°á»›i cÃ¢y
task.wait(2)
humanoidRootPart.CFrame = CFrame.new(targetPosition)
print("âœ… Teleport Ä‘áº¿n vá»‹ trÃ­ tÆ°á»›i cÃ¢y mÃ  khÃ´ng áº£nh hÆ°á»Ÿng Ä‘áº¿n camera!")

-- ğŸ“Œ Kiá»ƒm tra náº¿u nhÃ¢n váº­t bá»‹ rá»›t khá»i map, tá»± Ä‘á»™ng teleport vá» láº¡i trÆ°á»›c cá»•ng
task.spawn(function()
    while task.wait(2) do -- Kiá»ƒm tra má»—i 2 giÃ¢y
        if humanoidRootPart and humanoidRootPart.Position.Y < 10 then -- Náº¿u rÆ¡i xuá»‘ng quÃ¡ tháº¥p
            print("âš ï¸ PhÃ¡t hiá»‡n nhÃ¢n váº­t rá»›t khá»i map! Äang teleport láº¡i trÆ°á»›c cá»•ng...")
            teleportToStart()
        end
    end
end)
