local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

-- ğŸ“Œ Tá»a Ä‘á»™ Ä‘iá»ƒm trÆ°á»›c cá»•ng vÃ o map
local startPosition = Vector3.new(184.9508819580078, 22.715150833129883, 1995.9241943359375)

-- ğŸ“Œ Tá»a Ä‘á»™ vÃ o map (cÅ©ng lÃ  tá»a Ä‘á»™ tÆ°á»›i cÃ¢y)
local mapPosition = Vector3.new(-450.8958740234375, 108.41856384277344, -1392.82421875)

-- ğŸ“Œ HÃ m teleport nhÃ¢n váº­t Ä‘áº¿n trÆ°á»›c cá»•ng
local function teleportToStart()
    if humanoidRootPart then
        humanoidRootPart.CFrame = CFrame.new(startPosition)
        print("âœ… ÄÃ£ teleport Ä‘áº¿n trÆ°á»›c cá»•ng vÃ o map!")
    end
end

-- ğŸ“Œ Teleport ngay khi vÃ o game Ä‘áº¿n trÆ°á»›c cá»•ng map
teleportToStart()

-- ğŸ“Œ Chá» 1 giÃ¢y rá»“i teleport vÃ o trong map
task.wait(1)
humanoidRootPart.CFrame = CFrame.new(mapPosition)
print("ğŸš¶â€â™‚ï¸ ÄÃ£ vÃ o bÃªn trong map!")

-- ğŸ“Œ Chá»‰nh camera cao lÃªn Ä‘á»ƒ trÃ¡nh bá»‹ cáº£n
local camera = game.Workspace.CurrentCamera
task.wait(0.5)
camera.CFrame = camera.CFrame * CFrame.new(0, 10, -10) -- Äáº©y camera lÃªn cao hÆ¡n

-- ğŸ“Œ HÃ m tá»± Ä‘á»™ng tÆ°á»›i cÃ¢y (má»—i cÃ¢y tÆ°á»›i 4 láº§n)
local function waterPlants()
    local plantPositions = {
        Vector3.new(-452.69903564453125, 107.31900787353516, -1400.0970458984375),
        Vector3.new(-435.25531005859375, 107.33667755126953, -1399.5416259765625),
        Vector3.new(-435.50177001953125, 107.31939697265625, -1387.158447265625),
        Vector3.new(-435.40069580078125, 107.31939697265625, -1412.1187744140625)
    }

    for _, pos in ipairs(plantPositions) do
        for i = 1, 4 do
            local args = {
                [1] = Ray.new(Vector3.new(-478.3576354980469, 109.90526580810547, -1400.040283203125), pos - Vector3.new(-478.3576354980469, 109.90526580810547, -1400.040283203125)),
                [2] = pos
            }
            game:GetService("ReplicatedStorage").Network.Click:FireServer(unpack(args))
            task.wait(0.5)
        end
        print("ğŸŒ± ÄÃ£ tÆ°á»›i xong 4 láº§n cho cÃ¢y táº¡i:", pos)
    end
    print("ğŸŒ¿ ÄÃ£ tÆ°á»›i xong toÃ n bá»™ cÃ¢y!")
end

-- ğŸ“Œ Tá»± Ä‘á»™ng tÆ°á»›i cÃ¢y ngay khi teleport Ä‘áº¿n vá»‹ trÃ­ vÃ o map
task.wait(1)
waterPlants()

-- ğŸ“Œ Kiá»ƒm tra náº¿u nhÃ¢n váº­t bá»‹ rá»›t khá»i map, tá»± Ä‘á»™ng teleport láº¡i trÆ°á»›c cá»•ng
task.spawn(function()
    while task.wait(2) do
        if humanoidRootPart and humanoidRootPart.Position.Y < 10 then
            print("âš ï¸ PhÃ¡t hiá»‡n nhÃ¢n váº­t rá»›t khá»i map! Äang teleport láº¡i trÆ°á»›c cá»•ng...")
            teleportToStart()
        end
    end
end)
