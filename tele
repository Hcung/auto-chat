local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

-- ğŸ“Œ Tá»a Ä‘á»™ Ä‘iá»ƒm trÆ°á»›c cá»•ng vÃ o map
local startPosition = Vector3.new(184.9508819580078, 22.715150833129883, 1995.9241943359375)

-- ğŸ“Œ Tá»a Ä‘á»™ sau khi Ä‘Ã£ vÃ o map (sáº½ teleport tháº³ng)
local targetPosition = Vector3.new(-450.8958740234375, 108.41856384277344, -1392.82421875)

-- ğŸ“Œ HÃ m teleport nhÃ¢n váº­t Ä‘áº¿n trÆ°á»›c cá»•ng
local function teleportToStart()
    if humanoidRootPart then
        humanoidRootPart.CFrame = CFrame.new(startPosition)
        print("âœ… ÄÃ£ teleport Ä‘áº¿n trÆ°á»›c cá»•ng vÃ o map!")
    end
end

-- ğŸ“Œ Teleport ngay khi vÃ o game Ä‘áº¿n trÆ°á»›c cá»•ng map
teleportToStart()

-- ğŸ“Œ Chá» 1 giÃ¢y rá»“i báº¯t Ä‘áº§u Ä‘i bá»™ vÃ o map (chá»‰ 3 bÆ°á»›c)
task.wait(1)

print("ğŸš¶â€â™‚ï¸ Äang Ä‘i bá»™ vÃ o map...")
local moveSteps = {
    Vector3.new(184.9508819580078, 22.715150833129883, 1993), -- BÆ°á»›c 1
    Vector3.new(184.9508819580078, 22.715150833129883, 1990), -- BÆ°á»›c 2
    Vector3.new(184.9508819580078, 22.715150833129883, 1987)  -- BÆ°á»›c 3 (Dá»«ng táº¡i Ä‘Ã¢y)
}

for _, step in ipairs(moveSteps) do
    humanoid:MoveTo(step)
    humanoid.MoveToFinished:Wait()
    task.wait(0.3)
end

print("ğŸš¶â€â™‚ï¸ ÄÃ£ Ä‘i vÃ o trong map!")

-- ğŸ“Œ Dá»ªNG láº¡i, chá» 2 giÃ¢y rá»“i teleport Ä‘áº¿n chá»— tÆ°á»›i cÃ¢y
task.wait(2)
humanoidRootPart.CFrame = CFrame.new(targetPosition)
print("âœ… Teleport Ä‘áº¿n vá»‹ trÃ­ tÆ°á»›i cÃ¢y!")

-- ğŸ“Œ Danh sÃ¡ch lá»‡nh tÆ°á»›i cÃ¢y má»›i
