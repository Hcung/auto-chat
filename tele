-- ğŸ“Œ HÃ m tá»± Ä‘á»™ng tÆ°á»›i cÃ¢y (má»—i cÃ¢y 4 láº§n)
local function waterPlants()
    for _, clickData in ipairs(plantClickData) do
        for i = 1, 4 do -- TÆ°á»›i 4 láº§n trÆ°á»›c khi chuyá»ƒn cÃ¢y khÃ¡c
            local args = {
                [1] = clickData[1], -- Ray
                [2] = clickData[2]  -- Vá»‹ trÃ­ cÃ¢y
            }
            game:GetService("ReplicatedStorage").Network.Click:FireServer(unpack(args))
            task.wait(0.5) -- Chá» 0.5 giÃ¢y giá»¯a má»—i láº§n click
        end
        print("ğŸŒ± ÄÃ£ tÆ°á»›i xong 4 láº§n cho cÃ¢y táº¡i:", clickData[2])
    end
    print("ğŸŒ¿ ÄÃ£ tÆ°á»›i xong toÃ n bá»™ cÃ¢y!")
end

-- ğŸ“Œ Tá»± Ä‘á»™ng tÆ°á»›i cÃ¢y ngay khi teleport Ä‘áº¿n vá»‹ trÃ­ tÆ°á»›i
task.wait(1)
waterPlants()

-- ğŸ“Œ Kiá»ƒm tra náº¿u nhÃ¢n váº­t bá»‹ rá»›t khá»i map, tá»± Ä‘á»™ng teleport vá» láº¡i trÆ°á»›c cá»•ng
task.spawn(function()
    while task.wait(2) do
        if humanoidRootPart and humanoidRootPart.Position.Y < 10 then
            print("âš ï¸ PhÃ¡t hiá»‡n nhÃ¢n váº­t rá»›t khá»i map! Äang teleport láº¡i trÆ°á»›c cá»•ng...")
            teleportToStart()
        end
    end
end)
