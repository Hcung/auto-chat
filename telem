local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

-- ğŸ“Œ Tá»a Ä‘á»™ Ä‘iá»ƒm spawn ban Ä‘áº§u
local startPosition = Vector3.new(184.9508819580078, 22.715150833129883, 1995.9241943359375)

-- ğŸ“Œ Dá»‹ch chuyá»ƒn Ä‘áº¿n vá»‹ trÃ­ spawn ban Ä‘áº§u
humanoidRootPart.CFrame = CFrame.new(startPosition)
print("âœ… Teleport Ä‘áº¿n vá»‹ trÃ­ ban Ä‘áº§u thÃ nh cÃ´ng!")

-- ğŸ“Œ Chá» 1 giÃ¢y trÆ°á»›c khi di chuyá»ƒn Ä‘á»ƒ trÃ¡nh lá»—i
task.wait(1)

-- ğŸ“Œ Di chuyá»ƒn nhÃ¢n váº­t vá» phÃ­a trÆ°á»›c (3-4 bÆ°á»›c)
local moveDirection = Vector3.new(0, 0, -5) -- Äiá»u chá»‰nh vector Ä‘á»ƒ di chuyá»ƒn Ä‘Ãºng hÆ°á»›ng vÃ o map
for i = 1, 4 do
    humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(moveDirection)
    task.wait(0.5) -- Äá»£i má»™t chÃºt giá»¯a má»—i bÆ°á»›c Ä‘á»ƒ trÃ¡nh lá»—i dá»‹ch chuyá»ƒn quÃ¡ nhanh
