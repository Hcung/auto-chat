local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

-- ğŸ Toáº¡ Ä‘á»™ trÆ°á»›c Minigame
local minigamePosition = Vector3.new(183.913, 22.938, 1995.265)

-- ğŸš€ Teleport Ä‘áº¿n trÆ°á»›c Minigame
hrp.CFrame = CFrame.new(minigamePosition)
wait(1)

-- ğŸš¶ Äi vÃ i bÆ°á»›c vÃ o Minigame
for i = 1, 5 do
    hrp.CFrame = hrp.CFrame * CFrame.new(0, 0, -2)
    wait(0.5)
end

-- ğŸŒ± Tá»a Ä‘á»™ cÃ¡c cháº­u cÃ¢y
local teleportPositions = {
    Vector3.new(-464.85, 108.41, -1420.58),
    Vector3.new(-454.03, 108.80, -1420.85),
    Vector3.new(-439.38, 108.42, -1410.63),
    Vector3.new(-439.11, 108.42, -1399.78),
    Vector3.new(-442.27, 108.41, -1377.93),
    Vector3.new(-453.42, 108.80, -1377.46),
    Vector3.new(-451.87, 108.80, -1396.94),
    Vector3.new(-463.72, 108.80, -1377.45)
}

-- ğŸª£ TÆ°á»›i cÃ¢y (báº¥m chuá»™t trÃ¡i)
function waterPlant()
    mouse1click()
end

-- ğŸš€ Di chuyá»ƒn Ä‘áº¿n tá»«ng cháº­u vÃ  tÆ°á»›i cÃ¢y
function teleportAndWater()
    for _, pos in ipairs(teleportPositions) do
        hrp.CFrame = CFrame.new(pos) -- Teleport Ä‘áº¿n cháº­u cÃ¢y
        wait(0.3)
        waterPlant() -- Báº¥m chuá»™t tÆ°á»›i cÃ¢y
        wait(0.5) -- Äá»£i tÆ°á»›i xong rá»“i má»›i qua cÃ¢y khÃ¡c
    end
end

-- ğŸš€ Báº¯t Ä‘áº§u tÆ°á»›i cÃ¢y
teleportAndWater()
