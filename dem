local player = game.Players.LocalPlayer
local dataFolder = player:FindFirstChild("Inventory") or player:FindFirstChild("PlayerData") -- TÃ¬m thÆ° má»¥c chá»©a Keys

-- ğŸ“Œ HÃ m Ä‘áº¿m sá»‘ lÆ°á»£ng Secret Key
function countSecretKeys()
    if dataFolder then
        local keysFolder = dataFolder:FindFirstChild("Keys") -- TÃ¬m thÆ° má»¥c "Keys"
        if keysFolder then
            for _, item in ipairs(keysFolder:GetChildren()) do
                if string.find(item.Name, "Secret Key") then
                    print("ğŸ”‘ Sá»‘ lÆ°á»£ng Secret Key:", item.Value)
                    return item.Value
                end
            end
        end
    end
    print("âŒ KhÃ´ng tÃ¬m tháº¥y Secret Key!")
    return 0
end

-- ğŸš€ In sá»‘ lÆ°á»£ng Secret Key ra console
while true do
    countSecretKeys()
    wait(5) -- Cáº­p nháº­t má»—i 5 giÃ¢y
end
