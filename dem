local player = game.Players.LocalPlayer
local inventoryFolders = { "Backpack", "StarterGear", "Character" } -- CÃ¡c nÆ¡i cÃ³ thá»ƒ chá»©a item

-- ğŸ“Œ HÃ m in danh sÃ¡ch toÃ n bá»™ item trong inventory
function listAllItems()
    for _, folderName in ipairs(inventoryFolders) do
        local container = player:FindFirstChild(folderName)
        if container then
            for _, item in ipairs(container:GetChildren()) do
                print("ğŸ“¦ Item:", item.Name, "| Náº±m trong:", folderName)
            end
        end
    end
end

-- ğŸš€ Cháº¡y script Ä‘á»ƒ in danh sÃ¡ch item
listAllItems()
