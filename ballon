----------WebHook---------
local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
function send(url, embed)
    local headers = {
        ["Content-Type"] = "application/json"
    }
    local body = HttpService:JSONEncode({embeds = {embed}})
    local response = request({
        Url = url,
        Method = "POST",
        Headers = headers,
        Body = body
    })
    print("Sent")
end
local function aka()
    local localPlayer = Players.LocalPlayer
    local leaderstats = localPlayer:FindFirstChild("leaderstats")
    local diamonds = 0
    if leaderstats then
        local diamondsValue = leaderstats:FindFirstChild("\240\159\146\142 Diamonds")
        if diamondsValue then
            diamonds = diamondsValue.Value
        end
    end
    return diamonds
end
while true do
    pcall(function()
        local localPlayer = Players.LocalPlayer
        local username = localPlayer.Name
        local diamonds = aka()
        
        local embed = {
            ["title"] = "***ðŸŽˆDitBalloons PS99ðŸŽˆ***",
            ["description"] = "*Info-Account*",
            ["color"] = tonumber(0xffb4b4),
            ["fields"] = {
                {
                    ["name"] = "**ðŸ‘¨UsernameðŸ‘¨**",
