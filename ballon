                    ["inline"] = true,
                },
                {
                    ["name"] = "**ðŸ’ŽGemsðŸ’Ž**",
                    ["value"] = tostring(diamonds),
                    ["inline"] = true,
                }
            },
            ["footer"] = {
                ["icon_url"] = "https://media0.giphy.com/media/v1.Y2lkPTc5MGI3NjExNDM0aXhtczBzZjgwcHB6Y2M2ZWQzMXhicnM1cnlvZjRpemhrYWRkMiZlcD12MV9pbnRlcm5hbF9naWZfYnlfaWQmY3Q9Zw/9C1nyePnovqlpEYFMD/giphy.gif",
                ["text"] = "Dit Balloons (" .. os.date("%X") .. ")",
            }
        }
        if getgenv().cailonme.WebHook.Enable then
        send(getgenv().cailonme.WebHook.webhookURL, embed)
    end
    wait(getgenv().cailonme.WebHook.sendDelayMinutes * 60)
